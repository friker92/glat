global {
}

init {
}

void main() {
  start n0;
  trans n0 -> n1 {
  }

}

void m(){
   var int x;
   //var int y;

   start n0;

   trans n0 -> n1 {
     x := 5;
   }

   trans n1 -> n2 {
     assume(x<=7);
   }

   trans n2 -> n1 {
     x := x+1;
   }
   
   trans n1 -> n3 {
     assume(x>7);
   }
   trans n3 -> n4 {
     x := x+8;
   }
   
}
